<% if counttruemax > 0 %>
  <% if (1 or 21 or 31 or 41 or 51 or 61 or 71 or 81 or 91 or 101) == counttruemax %>
    <%= counttruemax %> правильный ответ (<%= th %> ч. <%= tm %> мин. <%= ts %> сек.)
  <% elsif (2..4 or 22..24 or 32..34 or 42..44 or 52..54 or 62..64 or 72..74 or 82..84 or 92..94) == counttruemax %>
    <%= counttruemax %> правильных ответа (<%= th %> ч. <%= tm %> мин. <%= ts %> сек.)
  <% else %>
    <%= counttruemax %> правильных ответов (<%= th %> ч. <%= tm %> мин. <%= ts %> сек.)
  <% end %>
<% end %>
